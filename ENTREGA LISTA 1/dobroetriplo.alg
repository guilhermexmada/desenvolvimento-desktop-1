algoritmo "dobro_e_triplo"

var
num1, num2, dobro_num1, triplo_num2 : inteiro
inicio
escreva ("Digite o primeiro número: ")
leia (num1)
escreva ("Digite o segundo número: ")
leia (num2)
dobro_num1 <- num1*2
triplo_num2 <- num2*3
escreval ("O dobro do primeiro número é ",dobro_num1)
escreval ("O triplo do segundo número é ",triplo_num2)
fimalgoritmo
